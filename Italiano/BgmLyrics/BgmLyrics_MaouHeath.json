    {
      "id": 0,
      "content": "<style=\"den_CP6_2\">Quanto ho desiderato un'altro",
      "vocalid": 0
    },
    {
      "id": 1,
      "content": "<style=\"den_CP6_2\">Futuro migliore, più allegro, felice",
      "vocalid": 0
    },
    {
      "id": 2,
      "content": "<style=\"den_CP6_2\">Eccomi, qui davanti al cancello",
      "vocalid": 0
    },
    {
      "id": 3,
      "content": "<style=\"den_CP6_2\">Cari vecchi tempi",
      "vocalid": 0
    },
    {
      "id": 4,
      "content": "<style=\"den_CP6_2\">Ho preso la mia decisione",
      "vocalid": 0
    },
    {
      "id": 5,
      "content": "<style=\"den_CP6_2\">Quanto avrai sofferto per la mia rabbia",
      "vocalid": 0
    },
    {
      "id": 6,
      "content": "<style=\"den_CP6_2\">Per il mio atteggiamento volgare, goffo",
      "vocalid": 0
    },
    {
      "id": 7,
      "content": "Quanto devi aver ignorato la tua meraviglia",
      "vocalid": 1
    },
    {
      "id": 8,
      "content": "Il tuo atteggiamento diretto, sincero",
      "vocalid": 1
    },
    {
      "id": 9,
      "content": "<style=\"den_CP6_2\">Eccoci qui, alla terra promessa",
      "vocalid": 0
    },
    {
      "id": 10,
      "content": "<style=\"den_CP6_2\">Vi metterò fine",
      "vocalid": 0
    },
    {
      "id": 11,
      "content": "Osserverai la mia fine?",
      "vocalid": 1
    },
    {
      "id": 12,
      "content": "<style=\"den_CP6_2\">Ooh",
      "vocalid": 0
    },
    {
      "id": 13,
      "content": "<style=\"den_CP6_2\">Ci giramo attorno",
      "vocalid": 0
    },
    {
      "id": 14,
      "content": "<style=\"den_CP6_2\">Tenendo stretto il dolore",
      "vocalid": 0
    },
    {
      "id": 15,
      "content": "<style=\"den_CP6_2\">Spinti dal nostro ego,",
      "vocalid": 0
    },
    {
      "id": 16,
      "content": "<style=\"den_CP6_2\">Sentimenti inespressi",
      "vocalid": 0
    },
    {
      "id": 17,
      "content": "<style=\"den_CP6_2\">Creando le nostre versioni del passato",
      "vocalid": 0
    },
    {
      "id": 18,
      "content": "<style=\"den_CP6_2\">Che non potresti mai conoscere",
      "vocalid": 0
    },
    {
      "id": 19,
      "content": "<style=\"den_CP6_2\">Perché so",
      "vocalid": 0
    },
    {
      "id": 20,
      "content": "<style=\"den_CP6_2\">Che devo essere la ragione per cui tu",
      "vocalid": 0
    },
    {
      "id": 21,
      "content": "<style=\"den_CP6_2\">Hai rinunciato a sorridere",
      "vocalid": 0
    },
    {
      "id": 22,
      "content": "<style=\"den_CP6_2\">E perché la speranza nei tuoi occhi venne rubata",
      "vocalid": 0
    },
    {
      "id": 23,
      "content": "<style=\"den_CP6_2\">So che devo essere la ragione per cui tu",
      "vocalid": 0
    },
    {
      "id": 24,
